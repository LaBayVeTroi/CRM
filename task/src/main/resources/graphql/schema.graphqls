input TaskInput {
    id: ID!
    title: String
    status: String
    priority: String
    dueDate: String
    createdOn: String
}

type Task {
    id: ID!
    title: String
    status: String
    priority: String
    dueDate: String
    createdOn: String
}

type TaskMutation {
    addNewTask : Task!
}

type TaskQuery {
    getAllTask : [Task]
}
